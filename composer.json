{
    "name": "kgilden/pager",
    "description": "A library to split results into multiple pages",
    "keywords": ["pager", "paginator", "pagination", "library"],
    "license": "MIT",
    "authors": [
        {
            "name": "Kristen Gilden",
            "email": "kristen.gilden@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "conflict": {
        "doctrine/orm": "<2.3",
        "symfony/http-foundation": "<2.4",
        "ruflin/elastica": "<0.90.1.0"
    },
    "require-dev": {
        "doctrine/orm": "^2.3",
        "symfony/http-foundation": "^2.4|^3.0",
        "ruflin/elastica": ">=0.90.1.0"
    },
    "autoload": {
        "psr-4": {"KG\\Pager\\": "src/"}
    },
    "suggest": {
        "doctrine/orm": "~2.3, if you're using DqlAdapter",
        "symfony/http-foundation": "~2.4 for automatic current page detection",
        "ruflin/elastica": ">=0.90.1.0 to use ElasticaAdapter",
        "ext-mongo": "to use MongoAdapter"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1-dev"
        }
    }
}
